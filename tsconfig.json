{
	"compilerOptions": {
		"strictNullChecks": true,
		"noImplicitAny": false,
		"sourceMap": true,
		"outDir": "./.dist/",
		"typeRoots": [
			"./node_modules/@types"
		],
		"module": "commonjs",
		"target": "es5",
		"jsx": "react"
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts"
	]
}
